<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  <%= f.engines_field_builder :username, horizontal: true %>
  <%= f.engines_field_builder :password, as: :password, horizontal: true %>
  <%= f.engines_submit({ text: 'Sign in', icon: 'fa-sign-in' }) %>
<% end %>
<%= render "users/shared/links" %>
