<%= content_tag :div, class: 'dashboard' do %>
  <%= content_tag :div, class: 'row' do %>
    <%= content_tag :div, class: 'col-sm-12' do %>
      <%= content_tag :div, class: 'display_inline pull_right_wide_media' do %>
        <%= return_to_system_link %>
      <% end %>
      <%= page_header text: 'Domains', icon: 'fa-globe' %>
    <% end %>
  <% end %>
  <%= content_tag :div, class: 'row' do %>
    <%= content_tag :div, class: 'col-sm-12 dashboard_containers' do %>
      <%= render @system.domains %>
    <% end %>
  <% end %>
<% end %>
